{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
    ],
    "settings": {
        "ecmascript": 6,
    },
    "rules": {
        "strict": 0,
        "no-unused-vars": 1,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "quotes": [1, "single"],
        "no-unused-expressions": 0,
        "no-use-before-define": 0,
        "no-loop-func": 1,
        "new-cap": 0,
        "curly": [2, "multi-line"]
    }
}
